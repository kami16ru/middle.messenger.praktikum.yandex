{
  "compilerOptions": {
    "target": "esnext",
//    "module": "es6",
    "moduleResolution": "node",
    "outDir": "dist",
    "noImplicitAny": true,
    "noUnusedLocals": true,
    "noUnusedParameters": true,
    "sourceMap": true,
    "strictNullChecks": true,
    "lib": [
      "es2019",
      "dom",
      "dom.iterable"
    ]
  },
  "include": [
    "./src/**/*.ts",
    "./index.d.ts"
  ],
  "exclude": [
    "./node_modules",
    "./dist"
  ],
